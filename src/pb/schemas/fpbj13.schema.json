{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Dados Assistenciais",
  "type": "object",
  "properties": {
    "dadosAssistenciais": {
      "description": "Conjunto de dados Assistenciais",
      "type": "object",
      "minProperties": 1,
      "properties": {
        "cns": {
          "description": "Carteira Nacional de Saúde",
          "type": "object",
          "minProperties": 1,
          "properties": {
            "numero": {
              "type": "string",
              "minLength": 1
            }
          }
        },
        "planosAssistenciais": {
          "description": "Histórico de planos assistenciais",
          "type": "object",
          "minProperties": 1,
          "properties": {
            "planoAssistencial": {
              "type": "array",
              "items": {
                "type": "object",
                "minProperties": 1,
                "properties": {
                  "descricaoProduto": {
                    "description": "Nome do produto registrado na ANS",
                    "type": "string",
                    "minLength": 1
                  },
                  "segmentacao": {
                    "type": "string",
                    "enum": [
                      "ASSISTENCIAL",
                      "ODONTOLOGICO"
                    ]
                  },
                  "statusProduto": {
                    "type": "string",
                    "enum": [
                      "SUSPENSO",
                      "ATIVO",
                      "CANCELADO"
                    ]
                  },
                  "opcao": {
                    "type": "integer"
                  },
                  "registroAns": {
                    "description": "Informações de registro do plano/produto na ANS",
                    "type": "object",
                    "minProperties": 1,
                    "properties": {
                      "registro": {
                        "type": "string",
                        "minLength": 1
                      }
                    }
                  },
                  "statusBeneficiario": {
                    "description": "Status da pessoa em relação ao plano/produto Capesesp",
                    "type": "string",
                    "enum": [
                      "ATIVO",
                      "SUSPENSO",
                      "DESLIGADO"
                    ]
                  },
                  "intervaloVinculo": {
                    "description": "Define um intervalo de vinculo da pessoa ao plano",
                    "type": "object",
                    "minProperties": 1,
                    "properties": {
                      "dataVinculo": {
                        "description": "Data de vinculo da pessoa ao plano",
                        "type": "string",
                        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
                      },
                      "dataDesligamento": {
                        "description": "Data de desligamento da pessoa ao plano. Não retornar para o plano atualmente vigente",
                        "type": [
                          "string",
                          "null"
                        ],
                        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
                      }
                    },
                    "required": [
                      "dataVinculo"
                    ]
                  },
                  "uf": {
                    "type": "string",
                    "enum": [
                      "AC",
                      "AL",
                      "AM",
                      "AP",
                      "BA",
                      "CE",
                      "DF",
                      "ES",
                      "GO",
                      "MA",
                      "MG",
                      "MS",
                      "MT",
                      "PA",
                      "PB",
                      "PE",
                      "PI",
                      "PR",
                      "RJ",
                      "RN",
                      "RO",
                      "RR",
                      "RS",
                      "SC",
                      "SE",
                      "SP",
                      "TO"
                    ]
                  },
                  "acomodacao": {
                    "type": [
                      "string",
                      "null"
                    ],
                    "enum": [
                      "ENFERMARIA",
                      "APARTAMENTO",
                      null
                    ]
                  },
                  "contrato": {
                    "type": "string",
                    "enum": [
                      "COLETIVO_EMPRESARIAL"
                    ]
                  },
                  "abrangencia": {
                    "type": "string",
                    "enum": [
                      "NACIONAL",
                      "GRUPO_DE_ESTADOS"
                    ]
                  },
                  "coberturaAns": {
                    "description": "Texto descritivo de cobertura padronizado pela ANS. Exemplo: 'AMBULATORIAL HOSPITALAR COM OBSTETRICIA'",
                    "type": "object",
                    "minProperties": 1,
                    "properties": {
                      "descricao": {
                        "type": "string",
                        "minLength": 1
                      }
                    },
                    "required": [
                      "descricao"
                    ]
                  },
                  "carencias": {
                    "type": "object",
                    "properties": {
                      "carencia": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "descricao": {
                              "type": "string",
                              "minLength": 1
                            },
                            "detalhes": {
                              "type": "string",
                              "minLength": 1
                            },
                            "dataTerminoCarencia": {
                              "type": "string",
                              "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
                            }
                          },
                          "required": [
                            "descricao",
                            "dataTerminoCarencia"
                          ]
                        }
                      }
                    }
                  },
                  "beneficios": {
                    "type": [
                      "object",
                      "null"
                    ],
                    "properties": {
                      "beneficio": {
                        "type": "array",
                        "items": {
                          "type": "string",
                          "enum": [
                            "AMUC",
                            "AUXILIO_FUNERAL",
                            "REEMBOLSO_MEDICAMENTO",
                            "DESCONTO_EM_MEDICAMENTO",
                            "ANTINEOPLASICO",
                            "CAPEPROTESE",
                            "OXIGENOTERAPIA",
                            "REEMBOLSO_LIVRE_ESCOLHA",
                            "INTERNACAO_DOMICILIAR"
                          ]
                        }
                      }
                    }
                  },
                  "programas": {
                    "type": [
                      "object",
                      "null"
                    ],
                    "properties": {
                      "programa": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "descricao": {
                              "type": "string",
                              "minLength": 1
                            },
                            "dataVinculo": {
                              "type": "string",
                              "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
                            }
                          },
                          "additionalProperties": false,
                          "required": [
                            "descricao",
                            "dataVinculo"
                          ]
                        }
                      }
                    },
                    "additionalProperties": false,
                    "required": [
                      "programa"
                    ]
                  },
                  "servicosComplementares": {
                    "type": [
                      "object",
                      "null"
                    ],
                    "properties": {
                      "servicoComplementar": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "descricao": {
                              "type": "string",
                              "minLength": 1
                            },
                            "dataVinculo": {
                              "type": "string",
                              "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
                            },
                            "dataDesligamento": {
                              "type": "string",
                              "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
                            }
                          },
                          "additionalProperties": false,
                          "required": [
                            "descricao",
                            "dataVinculo"
                          ]
                        }
                      }
                    }
                  },
                  "rescisao_contrato":{
                    "type": [
                      "object",
                      "null"
                    ],
                    "properties": {
                      "dataEntregaDocumento": {
                        "type": [
                          "string",
                          "null"
                        ],
                        "pattern": "^(|[0-9]{4}-[0-9]{2}-[0-9]{2})$"
                      }
                    }
                  },
                  "agregadoInstituidorFalecido": {
                    "type": "boolean"
                  }
                },
                "additionalProperties": false,
                "required": [
                  "descricaoProduto",
                  "statusBeneficiario",
                  "registroAns",
                  "segmentacao",
                  "intervaloVinculo",
                  "uf",
                  "contrato",
                  "abrangencia",
                  "coberturaAns",
                  "beneficios",
                  "agregadoInstituidorFalecido"
                ]
              }
            }
          },
          "additionalProperties": false,
          "required": [
            "planoAssistencial"
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "planosAssistenciais"
      ]
    },
    "statusExecucao": {
      "description": "Representa dados associados à execução de um método e seus possíveis retornos.",
      "type": "object",
      "minProperties": 1,
      "properties": {
        "executadoCorretamente": {
          "description": "'executadoCorretamente' indica se o método executou sem erros ou exceções internas. Não confundir 'executado corretamente' com 'invocado corretamente",
          "type": "boolean"
        },
        "mensagens": {
          "description": "Conjunto de mensagens de retorno",
          "type": "object",
          "minProperties": 1,
          "properties": {
            "mensagem": {
              "description": "Representa uma mensagem de retorno",
              "type": "array",
              "minItems": 1,
              "items": {
                "type": "object",
                "minProperties": 1,
                "properties": {
                  "severidade": {
                    "type": "string",
                    "enum": [
                      "WARN",
                      "INFO",
                      "SUCCESS",
                      "FATAL",
                      "ERROR"
                    ]
                  },
                  "mensagem": {
                    "description": "Texto resumido da mensagem (recomendável 60 a 80 caracteres)",
                    "type": "string",
                    "minLength": 1
                  },
                  "detalhes": {
                    "description": "Mensagem detalhada (quando aplicável)",
                    "type": "string",
                    "minLength": 1
                  },
                  "codigo": {
                    "description": "Código de de erro associado à mensagem",
                    "type": "number"
                  }
                },
                "required": [
                  "severidade",
                  "mensagem",
                  "codigo"
                ]
              }
            }
          }
        }
      },
      "required": [
        "executadoCorretamente",
        "mensagens"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "statusExecucao"
  ]
}
